   {
  "version": 2,
  "builds": [
    {
      "src": "/front_end/*",
      "use": "@vercel/static"
    },
    {
      "src": "back_end/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "front_end/index.html"
    }
  ]
}