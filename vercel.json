{
  "version": 2,
  "builds": [
    {
      "src": "front_end/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "front_end"
      }
    },
    {
      "src": "back_end/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "back_end/index.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "front_end/$1"
    }
  ]
}
